{
	"files.exclude": {
		"dist/": true,
		"frontend/": true,
	},
	"editor.tabSize": 2,
	"editor.defaultFormatter": "dbaeumer.vscode-eslint",
	"editor.formatOnSave": true,
	"typescript.preferences.importModuleSpecifier": "relative",
	"eslint.format.enable": true,
